{
  "name": "@grace-studio/next-umbraco",
  "version": "0.0.1",
  "description": "Middle layer to connect a Next.js application with Umbraco Delivery API.",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "preversion": "npm test && npm run lint",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grace-studio/next-umbraco.git"
  },
  "keywords": [],
  "author": "Grace Studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grace-studio/next-umbraco/issues"
  },
  "homepage": "https://github.com/grace-studio/next-umbraco#readme",
  "devDependencies": {
    "@types/jest": "29.5.13",
    "@types/node": "22.7.5",
    "@types/qs": "6.9.16",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "prettier-eslint": "16.3.0",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3"
  },
  "dependencies": {
    "deepmerge": "4.3.1",
    "qs": "6.13.0"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "node": ">=20"
}
